// Radarapp iOS - Configuration

// API Configuration
API_BASE_URL_DEV = http:/$()/localhost:8000
API_BASE_URL_PROD = https:/$()/radarapp.fr
API_VERSION = v1

// App Info
PRODUCT_BUNDLE_IDENTIFIER_DEV = fr.radarapp.ios.dev
PRODUCT_BUNDLE_IDENTIFIER_PROD = fr.radarapp.ios
PRODUCT_NAME = Radarapp
MARKETING_VERSION = 1.0.0
CURRENT_PROJECT_VERSION = 1

// Build Settings
DEVELOPMENT_TEAM = YOUR_TEAM_ID
CODE_SIGN_STYLE = Automatic
TARGETED_DEVICE_FAMILY = 1,2
IPHONEOS_DEPLOYMENT_TARGET = 17.0
SWIFT_VERSION = 6.0
ENABLE_PREVIEWS = YES

// Feature Flags
ENABLE_DEBUG_LOGGING_DEV = YES
ENABLE_DEBUG_LOGGING_PROD = NO
USE_MOCK_DATA_DEV = NO
USE_MOCK_DATA_PROD = NO
